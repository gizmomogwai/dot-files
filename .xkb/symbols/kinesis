partial modifier_keys
xkb_symbols "mylayout" {

  // additional ways to ebene 4
  replace key <TLDE> {
    [ NoSymbol, NoSymbol ]
  };
  replace key <AD99> {
    type[Group1] = "ONE_LEVEL",
    symbols[Group1] = [ ISO_Level4_Shift ]
  };

  modifier_map Mod3 { <TLDE> };


  // meta keys on end and apple
  replace key <RALT> {
    [ Super_L, Super_L ]
  };
  replace key <END> {
    [ Super_R, Super_R ]
  };
  modifier_map Mod4 { <RALT>, <END> };

};
